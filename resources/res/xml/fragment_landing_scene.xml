<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android" xmlns:motion="http://schemas.android.com/apk/res-auto">
    <ConstraintSet android:id="@+id/start">
        <Constraint android:id="@+id/star2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="8dp" android:layout_marginEnd="70dp" motion:layout_constraintBottom_toBottomOf="@+id/guideline_horizontal_top" motion:layout_constraintEnd_toEndOf="0"/>
        <Constraint android:id="@+id/star5" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:layout_marginEnd="50dp" motion:layout_constraintEnd_toEndOf="0" motion:layout_constraintStart_toStartOf="@+id/guideline_vertical_end" motion:layout_constraintTop_toBottomOf="@+id/guideline_horizontal_top"/>
        <Constraint android:id="@+id/star4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:alpha="0.1" motion:layout_constraintEnd_toEndOf="@+id/guideline_vertical_start" motion:layout_constraintTop_toBottomOf="@+id/guideline_horizontal_top" motion:layout_editor_absoluteX="11dp"/>
        <Constraint android:id="@+id/star6" android:layout_width="wrap_content" android:layout_height="wrap_content" android:alpha="0.1" android:layout_marginEnd="65dp" motion:layout_constraintEnd_toEndOf="0" motion:layout_constraintTop_toBottomOf="@+id/landing_toolbar"/>
        <Constraint android:id="@+id/star7" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="24dp" android:layout_marginEnd="16dp" motion:layout_constraintEnd_toEndOf="@+id/guideline_vertical_start" motion:layout_constraintTop_toBottomOf="@+id/star4" motion:layout_editor_absoluteX="15dp"/>
        <Constraint android:id="@+id/star3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="10dp" motion:layout_constraintBottom_toTopOf="@+id/guideline_horizontal_top" motion:layout_constraintStart_toEndOf="@+id/star4"/>
    </ConstraintSet>
    <ConstraintSet android:id="@+id/end">
        <Constraint android:id="@+id/star3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="70dp" android:layout_marginStart="50dp" motion:layout_constraintBottom_toTopOf="@+id/guideline_horizontal_top" motion:layout_constraintStart_toEndOf="@+id/star4"/>
        <Constraint android:id="@+id/star7" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="24dp" motion:layout_constraintEnd_toEndOf="@+id/guideline_vertical_start" motion:layout_constraintTop_toBottomOf="@+id/star4"/>
    </ConstraintSet>
    <Transition motion:autoTransition="animateToEnd" motion:constraintSetEnd="@+id/end" motion:constraintSetStart="@+id/start" motion:duration="3000"/>
</MotionScene>
